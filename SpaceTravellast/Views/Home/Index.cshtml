<!-- hidden navigation bar -->
<!-- / hidden bar -->
<!-- banner section -->
<section class="banner-section">
    <div class="banner-container">
        <div class="banner-arrow wow slideinleft" data-wow-delay="0ms" data-wow-duration="1500ms"><img src="~/images/background/banner-arrow.png" alt="" title=""></div>
        <div class="auto-container">
            <div class="row clearfix">
                <div class="left-col col-lg-6 col-md-12">
                    <div class="inner">
                        <div class="clearfix">
                            <div class="content">
                                <div class="bg-image"><img src="~/images/background/bg-gradient-1.png" alt="" title=""></div>
                                <h3>@localizer["start travelling now"]</h3>
                                <h1 style="font-family: 'Roboto', sans-serif;"><i class="d-icon"><img src="~/images/resource/bde-1.png" alt="" title=""></i> @localizer["Explore the top destination of planet earth"]</h1>
                                <div class="text" style="font-family: 'Roboto', sans-serif;">@localizer["Make your tour more enjoyable with Space Travel"]</div>

                                <div class="lower-text"><span>@ViewBag.turlar.Count</span> @localizer["tour packages waiting for you"]</div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="right-col col-lg-6 col-md-12">
                    <div class="inner wow fadeinright" data-wow-delay="0ms" data-wow-duration="1500ms">
                        <div class="image-layer" style="background-image: url(../images/background/banner-1.jpg);"></div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>
<!--end banner section -->
<!--intro section-->
<section class="intro-section">
    <div class="auto-container">
        <div class="row clearfix justify-content-center">
            <!--block-->
            <div class="intro-block col-lg-4 col-md-6 col-sm-12">
                <div class="inner-box wow fadeinup" data-wow-delay="0ms" data-wow-duration="1500ms">
                    <div class="icon"><span><img src="~/images/resource/intro-1.png" alt=""></span></div>
                    <h4 style="font-family: 'Roboto', sans-serif;font-weight: 400;">@localizer["Most popular destination"]</h4>
                    <div class="text">@localizer["Those planning a comfortable and unforgettable vacation in hotels located in the heart of nature, on the coast of the blue sea, can contact Space Travel"]</div>
                </div>
            </div>
            <!--block-->
            <div class="intro-block col-lg-4 col-md-6 col-sm-12">
                <div class="inner-box wow fadeinup" data-wow-delay="300ms" data-wow-duration="1500ms">
                    <div class="icon"><span><img src="~/images/resource/intro-2.png" alt=""></span></div>
                    <h4 style="font-family: 'Roboto', sans-serif;font-weight: 400;">@localizer["Budget friendly packages"]</h4>
                    <div class="text">@localizer["Every detail is thought out for the comfort of children and adults, suitable for every budget, family holidays are organized with direct flights, comfortable hotels located in the center, transfers between cities and, most importantly, visa support."]</div>
                </div>
            </div>
            <!--block-->
            <div class="intro-block col-lg-4 col-md-6 col-sm-12">
                <div class="inner-box wow fadeinup" data-wow-delay="600ms" data-wow-duration="1500ms">
                    <div class="icon"><span><img src="~/images/resource/intro-3.png" alt=""></span></div>
                    <h4 style="font-family: 'Roboto', sans-serif;font-weight: 400;">@localizer["Satisfaction guarranted"]</h4>
                    <div class="text">
                        @localizer["Customer satisfaction is the main priority in Space Travel. That's why our team works day and night to provide our customers with the best service."]
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<!--about us section-->
