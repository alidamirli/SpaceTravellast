<!--packages section-->
<section class="packages-section">
    <div class="bg-layer" style="background-image: url(../images/background/pattern-1.png);"></div>
    <div class="auto-container">
        <div class="title-box">
            <div class="subtitle">@localizer["Packages"]</div>
            <h2><span style="font-family: 'Roboto', sans-serif;font-weight: 500;">@localizer["Travel Packages"]</span></h2>
        </div>
        <div class="carousel-box">
            <div class="packages-carousel">
                @foreach (Turlar item in ViewBag.turlar)
                {
                    <!--block-->
                    <div class="package-block">
                        <div class="inner-box">
                            <div class="image-box">
                                <div class="image"><a href="~/Home/TourDetails/@item.TurlarId"><img src="~/images/Small/@item.TurlarFirstphoto" alt=""></a></div>
                                <div class="b-title top-rated"><span>@item.TurlarCategory.CategoryName</span></div>
                            </div>
                            <div class="lower-box">
                                <div class="p-icon"><img src="~/images/resource/t-icon-1.png" alt=""><span class="icon flaticon-adventure"></span></div>
                                <div class="location">@item.TurlarCountry.CountryName</div>
                                <h5><a href="~/Home/TourDetails/@item.TurlarId">@item.TurDescription.Substring(0,150)</a></h5>
                                <div class="info clearfix">
                                    <div class="duration"><i class="fa fa-clock"></i> @item.TurTarix</div>
                                    <div class="persons"><i class="fa fa-user"></i> 1</div>
                                </div>
                                <div class="bottom-box clearfix">
                                    <div class="rating"><a href="#" class="theme-btn"><i class="fa fa-star"></i> <strong>4.8</strong> &ensp; <span class="count">3210 reviews</span></a></div>
                                    <div class="price">@localizer["start from"] &ensp;<span class="amount">@item.TurlarCurrency.CurrencyName@item.TurPrice</span></div>
                                </div>
                            </div>
                        </div>
                    </div>

                }
            </div>
        </div>
    </div>
</section>

