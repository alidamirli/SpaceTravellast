                                        <li><a href="#"><i class="fa fa-folder"></i> adventure</a></li>
                                        <li><a href="#"><i class="fa fa-clock"></i> @item.NewsTime</a></li>
                                        @* <li><a href="#"><i class="fa fa-comments"></i>@item.Comments.Count</a></li> *@
                                    </ul>
                                    <h3><a href="~/Home/NewsDetails/@item.NewsId">@item.NewsName</a></h3>
                                </div>
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
</section>

